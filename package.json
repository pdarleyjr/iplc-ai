{
  "name": "iplc-ai",
  "version": "1.0.0",
  "description": "IPLC AI Worker for RAG and document processing",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev src/index.ts --port 8788 --local",
